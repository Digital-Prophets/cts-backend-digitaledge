{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "DigitalEdge.Web": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "progress": false,
            "extractCss": true,
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/gentelella/vendors/bootstrap/dist/css/bootstrap.min.css",
              "src/gentelella/vendors/font-awesome/css/font-awesome.min.css",
              "src/gentelella/vendors/nprogress/nprogress.css",
              "src/gentelella/vendors/iCheck/skins/flat/green.css",
              "src/gentelella/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css",
              "src/gentelella/vendors/jqvmap/dist/jqvmap.min.css",
              "src/gentelella/vendors/bootstrap-daterangepicker/daterangepicker.css",
              "src/gentelella/build/css/custom.min.css",
              "node_modules/ngx-toastr/toastr.css",
              "src/styles.css"
            ],
            "scripts": [
              "src/gentelella/vendors/jquery/dist/jquery.min.js",
              "src/gentelella/vendors/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/gentelella/vendors/fastclick/lib/fastclick.js",
              "src/gentelella/vendors/nprogress/nprogress.js",
              "src/gentelella/vendors/Chart.js/dist/Chart.min.js",
              "src/gentelella/vendors/gauge.js/dist/gauge.min.js",
              "src/gentelella/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js",
              "src/gentelella/vendors/iCheck/icheck.min.js",
              "src/gentelella/vendors/skycons/skycons.js",
              "src/gentelella/vendors/Flot/jquery.flot.js",
              "src/gentelella/vendors/Flot/jquery.flot.pie.js",
              "src/gentelella/vendors/Flot/jquery.flot.time.js",
              "src/gentelella/vendors/Flot/jquery.flot.stack.js",
              "src/gentelella/vendors/Flot/jquery.flot.resize.js",
              "src/gentelella/vendors/flot.orderbars/js/jquery.flot.orderBars.js",
              "src/gentelella/vendors/flot-spline/js/jquery.flot.spline.min.js",
              "src/gentelella/vendors/flot.curvedlines/curvedLines.js",
              "src/gentelella/vendors/DateJS/build/date.js",
              "src/gentelella/vendors/jqvmap/dist/jquery.vmap.js",
              "src/gentelella/vendors/jqvmap/dist/maps/jquery.vmap.world.js",
              "src/gentelella/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js",
              "src/gentelella/vendors/moment/min/moment.min.js",
              "src/gentelella/vendors/bootstrap-daterangepicker/daterangepicker.js",
              "src/gentelella/build/js/custom.min.js"
            ],
            "aot": false,
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "DigitalEdge.Web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "DigitalEdge.Web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "DigitalEdge.Web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist-server",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.server.json",
            "sourceMap": true,
            "optimization": false
          },
          "configurations": {
            "dev": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": true
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false
            }
          },
          "defaultConfiguration": ""
        }
      }
    },
    "DigitalEdge.Web-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "DigitalEdge.Web:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "DigitalEdge.Web",
  "cli": {
    "analytics": "fe08dd44-cb96-495e-bac2-d7331722f275"
  }
}